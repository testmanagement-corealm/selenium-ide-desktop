{
  "name": "selenium-ide",
  "version": "4.0.1-beta.14",
  "private": false,
  "description": "Selenium IDE electron app",
  "author": "Todd <tarsitodd@gmail.com>",
  "homepage": "http://github.com/SeleniumHQ/selenium-ide",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf build dist files node_modules tsconfig.tsbuildinfo",
    "start": "electron build/main-bundle.js",
    "dev": "SIDE_DEV=1 electron build/main-bundle.js",
    "build": "run-s build:webpack build:electron",
    "build:electron": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -mwl --publish never",
    "build:electron:dir": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --dir",
    "build:electron:mac": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -m --publish never",
    "build:webpack": "webpack",
    "install-app-deps": "electron-builder install-app-deps",
    "publish:electron:linux": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -l --publish always",
    "publish:electron:mac": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -m --publish always",
    "publish:electron:windows": "electron-builder -w --publish always",
    "release": "electron-builder",
    "serve": "SIDE_DEV=1 webpack serve",
    "watch": "webpack watch"
  },
  "bin": {
    "selenium-ide": "bin.js"
  },
  "sideEffects": false,
  "main": "build/main-bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeleniumHQ/selenium-ide.git"
  },
  "bugs": {
    "url": "https://github.com/SeleniumHQ/selenium-ide/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "build": {
    "appId": "org.seleniumhq.selenium-ide",
    "asarUnpack": [
      "node_modules/electron-chromedriver/bin/chromedriver",
      "node_modules/electron-chromedriver/bin/chromedriver.exe"
    ],
    "files": [
      "**/build/**/*",
      "**/packages/*/dist/**/*.js",
      "!**/*.d.ts",
      "!**/selenium-ide/dist/**/*"
    ],
    "directories": {
      "buildResources": "resources"
    }, 
    "productName": "RTA DESKTOP",
    "mac": {
      "target": {
        "target": "default",
        "arch": "universal"
      },
      "singleArchFiles": "*",
      "x64ArchFiles": "*",
      "mergeASARs": false,
      "category": "public.app-category.developer-tools",
      "darkModeSupport": true
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": [
        {
          "provider": "github",
          "owner": "SeleniumHQ",
          "repo": "selenium-ide"
        }
      ],
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "target": "AppImage"
    },
    "nodeVersion": "18.x",
    "fileAssociations": [
      {
        "ext": "side",
        "name": "Selenium IDE 3 project",
        "role": "Editor"
      },
      {
        "ext": "sideproj",
        "name": "Selenium IDE project",
        "role": "Editor"
      }
    ]
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@seleniumhq/code-export-csharp-nunit": "4.0.12",
    "@seleniumhq/code-export-csharp-xunit": "4.0.12",
    "@seleniumhq/code-export-java-junit": "4.0.12",
    "@seleniumhq/code-export-javascript-mocha": "4.0.12",
    "@seleniumhq/code-export-python-pytest": "4.0.12",
    "@seleniumhq/code-export-ruby-rspec": "4.0.12",
    "@seleniumhq/get-driver": "4.0.12",
    "@seleniumhq/side-api": "4.0.12",
    "@seleniumhq/side-commons": "4.0.12",
    "@seleniumhq/side-model": "4.0.12",
    "@seleniumhq/side-runtime": "4.0.12",
    "dnd-core": "^16.0.1",
    "electron-chromedriver": "^28.0.0",
    "electron-log": "^5.1.0",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^6.1.7",
    "fs-extra": "^11.1.1",
    "lodash": "4.17.21",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.5",
    "react-resizable-panels": "^1.0.8",
    "scroll-into-view-if-needed": "^3.0.10",
    "selenium-webdriver": "^4.20.0",
    "side-code-export": "^4.0.11",
    "v8-compile-cache": "^2.3.0"
  },
  "devDependencies": {
    "@medv/finder": "^1.1.1",
    "@seleniumhq/browser-info": "4.0.12",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/electron-devtools-installer": "^2.2.5",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/selenium-webdriver": "^4.1.22",
    "@types/webpack-env": "^1.16.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.8.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.1.6",
    "source-map-loader": "^4.0.1",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
